{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(LocalDB)\\MSSQLLocalDB;Database=UserDB;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "AppConfig": {
    "Github": {
      "BaseAddress": "https://api.github.com",
      "ProfileUrl": "users/{0}",
      "RepoUrl": "users/{0}/repos",
      "Timeout": 45
    }
  },
  "JaegerConfig": {
    "Host": "localhost",
    "Port": 6831,
    "IsEnabled": true,
    "SamplingRate": 0.5
  }
}
